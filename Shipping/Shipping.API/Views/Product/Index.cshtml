@model IEnumerable<ApplicationCore.Entities.Product>

<!-- customer view -->
<h1>List of Products</h1>
<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>ProductName</th>
        <th>Description</th>
        <th>Price</th>
        <td></td>
    </tr>
    </thead>
    <tbody>
    @foreach(var m in Model)
    {
        <tr>
            <td>@m.Id</td>
            <td>@m.ProductName</td>
            <td>@m.Description</td>
            <td>@m.Price</td>

        </tr>
    }
    </tbody>
</table>